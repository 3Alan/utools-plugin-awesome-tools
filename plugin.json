{
  "author": "Alan",
  "homepage": "https://github.com/3Alan",
  "pluginName": "awesome-plugin",
  "description": "这是一个工具库",
  "logo": "logo.png",
  "main": "index.html",
  "version": "0.0.1",
  "development": {
    "main": "http://127.0.0.1:3000/index.html"
  },
  "features": [
    {
      "code": "browserCode",
      "explain": "userAgent",
      "cmds": ["browser"]
    },
    {
      "code": "niceRegex",
      "explain": "niceRegex",
      "cmds": [
        {
          "type": "regex",
          "label": "文本正则匹配",
          "match": "/0?(13|14|15|18|17)[0-9]{9}/"
        }
      ]
    }
  ]
}
